             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:GraficadorSeñales"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        <TextBlock HorizontalAlignment="Left" Margin="10,2,0,0" TextWrapping="Wrap" Text="Alpha:" VerticalAlignment="Top" RenderTransformOrigin="-5.71,-12.287"/>
        <TextBox x:Name="txtAlpha" HorizontalAlignment="Left" Height="18" Margin="10,19,0,0" TextWrapping="Wrap" Text="1" VerticalAlignment="Top" Width="77"/>
    </Grid>
</UserControl>
